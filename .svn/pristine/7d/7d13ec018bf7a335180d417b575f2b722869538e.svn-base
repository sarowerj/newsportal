/*================ Base for label styling=============*/
.css_checkbox:not(:checked),
.css_checkbox:checked {
    position: absolute;
    left: -9999px;
}
.css_checkbox:not(:checked) + label,
.css_checkbox:checked + label {
    position: relative;
    padding-left: 25px;
    cursor: pointer;
    -webkit-user-select: none;
    -moz-user-select:none;
    -ms-user-select:none;
    user-select:none;
}

/* checkbox aspect */
.css_checkbox:not(:checked) + label:before,
.css_checkbox:checked + label:before {
    content: '';
    position: absolute;
    left:0; top: 2px;
    width: 17px; height: 17px;
    border: 1px solid #aaa;
    background: #f8f8f8;
}
/* checked mark aspect */
.css_checkbox:not(:checked) + label:after,
.css_checkbox:checked + label:after {
    content: '';
    position: absolute;
    top: 5px; 
    left: 3px;
    width:11px;
    height:11px;
    background:#3c8dbc;
    font-size: 14px;
    line-height: 0.8;
    color: #09ad7e;
    transition: all .2s;
}
/* checked mark aspect changes */
.css_checkbox:not(:checked) + label:after {
    opacity: 0;
    transform: scale(0);
}
.css_checkbox:checked + label:after {
    opacity: 1;
    transform: scale(1);
}
/* disabled checkbox */
.css_checkbox:disabled:not(:checked) + label:before,
.css_checkbox:disabled:checked + label:before {
    box-shadow: none;
    border-color: #bbb;
    background-color: #ddd;
}
.css_checkbox:disabled:checked + label:after {
    color: #999;
}
.css_checkbox:disabled + label {
    color: #aaa;
}
/* accessibility */
.css_checkbox:checked:focus + label:before,
.css_checkbox:not(:checked):focus + label:before {
    border: 1px dotted blue;
}

/* hover style just for information */
label:hover:before {
    border: 1px solid #4778d9!important;
}


