/*================== Media Modal Styling ==================*/
.modal .tab-pane {
    max-height: 420px;
    overflow-y: auto;
    overflow-x:hidden;
}
.media_modal ul{
    padding-left:0;
}
.media_modal ul li label,.media_modal ul li img{
    width:100%;
}
.media_modal li{
    list-style: none;
}
.media_modal .tab-pane ul li{
    margin-bottom: 30px;
}
.media_modal .item{
    margin-bottom: 30px;
    cursor:pointer;
    border:0px;
    position:relative;
    transition:all .1s;
    height: 120px;
    display: table-cell;
    vertical-align: middle;
    background:#f1f1f1;
}
.media_modal .item .selected_icon{
    position:absolute;
    right:0;
    bottom:0;
    background:#3c8dbc;
    color:white;
    opacity: 0;
    transition:all .25s;    
}
.media_modal .item .selected_icon .glyphicon{
    display:block;
    padding:10px;
}
.media_modal .item.selected{
    border:5px solid #3c8dbc;
}
.media_modal .item.selected .selected_icon{
    opacity:1;
}
.media_modal .tab-pane{
    padding-top:30px;
}
