<div class="tab-pane active" id="tab_a">
    <h4>Student's Information</h4>
    <div class="col-sm-12">
        <div class="form-group">
            <label class="col-md-4 control-label" for="st_name_bangla">Student's Name(Bangla)</label>  
            <div class=" col-sm-7 col-xs-12">
                <input id="st_name_bangla" name="st_name_bangla" type="text" placeholder="Student's name(Bangla)" class="form-control input-md" >
            </div>
        </div>
        <div class="form-group">
            <label class="col-md-4 control-label" for="st_name_english">Student's Name(English)</label>  
            <div class=" col-sm-7 col-xs-12">
                <input id="st_name_english" name="st_name_english" type="text" placeholder="Student's name(English)" class="form-control input-md" >
            </div>
        </div>
        <div class="form-group">
            <label class="col-md-4 control-label" for="id_no">Birth Certificate No</label>  
            <div class="col-md-7">
                <input id="bitrh_certificate" name="bitrh_certificate" type="text" placeholder="Birth Certificate No" class="form-control input-md"data-inputmask='"mask": "99999999999999999999"' data-mask>
            </div>
        </div>
        <div class="form-group">
            <label class="col-md-4 control-label" for="st_phone">Student's Phone</label>  
            <div class=" col-sm-7 col-xs-12">
                <input id="st_phone" name="st_phone" type="text" placeholder="Student's Phone" class="form-control input-md" data-inputmask='"mask": "99999999999"' data-mask>
            </div>
        </div>
        <div class="form-group">
            <label class="col-md-4 control-label" for="st_email">Student's Email</label>  
            <div class=" col-sm-7 col-xs-12">
                <input id="st_email" name="st_email" type="email" placeholder="Student's Email" class="form-control input-md" >
            </div>
        </div>
        <div class="form-group">
            <label class="col-md-4 control-label" for="date_of_birth">Date of Birth</label>  
            <div class="  col-sm-7 col-xs-12">
                <input type="text" name="date_of_birth" id="date_of_birth" class="form-control" data-inputmask="'alias': 'dd/mm/yyyy'" data-mask>
            </div>
        </div>
        <div class="form-group">
            <label class="col-md-4 control-label" for="nationality">Nationality</label>  
            <div class="  col-sm-7 col-xs-12">
                <select name="nationality" id="nationality" class="form-control select2" data-placeholder="Select Nationality" style="width: 100%;">
                    <option value="Bangladeshi">Bangladeshi</option>
                    <option value="Foreigner">Foreigner</option>
                </select>
            </div>
        </div>

        <div class="form-group">
            <label class="col-md-4 control-label" for="religion">Religion</label>  
            <div class="  col-sm-7 col-xs-12">
                <select name="religion" id="religion" class="form-control select2" data-placeholder="Select Religion" style="width: 100%;">
                    <option value="Islam">Islam</option>
                    <option value="Hindu">Hindu</option>
                    <option value="Other">Other</option>
                </select>
            </div>
        </div>

        <div class="form-group">
            <label class="col-md-4 control-label" for="blood_group">Blood Broup</label>  
            <div class="  col-sm-7 col-xs-12">
                <select name="blood_group" id="blood_group" class="form-control  select2" data-placeholder="Select Blood Group" style="width: 100%;">
                    <option value="A+">A+</option>
                    <option value="A-">A-</option>
                    <option value="B+">B+</option>
                    <option value="B-">B-</option>
                    <option value="O+">O+</option>
                    <option value="O-">O-</option>
                    <option value="AB+">AB+</option>
                    <option value="AB-">AB-</option>
                </select>
            </div>
        </div>
    </div>

</div>
<script>
    $(function () {
        $("#datemask").inputmask("dd/mm/yyyy", {"placeholder": "dd/mm/yyyy"});
        $("[data-mask]").inputmask();
    });
</script>