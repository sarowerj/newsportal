<div class="tab-pane active" id="tab_a">
    <h4>Patent's Information</h4>
    <div class="col-sm-12">
        <fieldset>
            <legend>Father</legend>
        <div class="form-group">
            <label class="col-md-4 control-label" for="ft_name_bangla">Father's Name(Bangla)</label>  
            <div class="col-md-7">
                <input id="ft_name_bangla" name="ft_name_bangla" type="text" placeholder="Father's name(Bangla)" class="form-control input-md">
            </div>
        </div>
        <div class="form-group">
            <label class="col-md-4 control-label" for="ft_name_english">Father's Name(English)</label>  
            <div class="col-md-7">
                <input id="ft_name_english" name="ft_name_english" type="text" placeholder="Father's name(English)" class="form-control input-md">
            </div>
        </div>
        <div class="form-group">
            <label class="col-md-4 control-label" for="ft_phone">Father's Phone</label>  
            <div class="col-md-7">
                <input id="ft_phone" name="ft_phone" type="text" placeholder="Father's Phone" class="form-control input-md" data-inputmask='"mask": "99999999999"' data-mask>
            </div>
        </div>
        <div class="form-group">
            <label class="col-md-4 control-label" for="ft_occupation">Father's Occupation</label>  
            <div class="col-md-7">
                <input id="ft_occupation" name="ft_occupation" type="text" placeholder="Father's Occupation" class="form-control input-md">
            </div>
        </div>
        <div class="form-group">
            <label class="col-md-4 control-label" for="ft_occupation">Father's Designation</label>  
            <div class="col-md-7">
                <input id="ft_designation" name="ft_designation" type="text" placeholder="Father's Designation" class="form-control input-md">
            </div>
        </div>
        <div class="form-group">
            <label class="col-md-4 control-label" for="ft_occupation">Is Contact Person</label>  
            <div class="col-md-7">
                <input id="contact_person" class="contact-person" name="contact_person_f" type="radio" value="1"> Yes
                <input id="contact_person" class="contact-person" name="contact_person_f" checked="" type="radio" value="0"> No
            </div>
        </div>
        </fieldset>
        <fieldset>
            <legend>Mother</legend>
        <div class="form-group">
            <label class="col-md-4 control-label" for="moth_name_bangla">Mother's Name(Bangla)</label>  
            <div class="col-md-7">
                <input id="moth_name_bangla" name="moth_name_bangla" type="text" placeholder="Mother's name(Bangla)" class="form-control input-md">
            </div>
        </div>
        <div class="form-group">
            <label class="col-md-4 control-label" for="moth_name_english">Mother's Name(English)</label>  
            <div class="col-md-7">
                <input id="moth_name_english" name="moth_name_english" type="text" placeholder="Mother's name(English)" class="form-control input-md">
            </div>
        </div>
        <div class="form-group">
            <label class="col-md-4 control-label" for="moth_phone">Mother's Phone</label>  
            <div class="col-md-7">
                <input id="moth_phone" name="moth_phone" type="text" placeholder="Mother's Phone" class="form-control input-md" data-inputmask='"mask": "99999999999"' data-mask>
            </div>
        </div>
        <div class="form-group">
            <label class="col-md-4 control-label" for="moth_occupation">Mother's Occupation</label>  
            <div class="col-md-7">
                <input id="moth_occupation" name="moth_occupation" type="text" placeholder="Mother's Occupation" class="form-control input-md">
            </div>
        </div>
        <div class="form-group">
            <label class="col-md-4 control-label" for="moth_occupation">Mother's Designation</label>  
            <div class="col-md-7">
                <input id="moth_occupation" name="moth_occupation" type="text" placeholder="Mother's Designation" class="form-control input-md">
            </div>
        </div>
        <div class="form-group">
            <label class="col-md-4 control-label" for="ft_occupation">Is Contact Person</label>  
            <div class="col-md-7">
                <input id="contact_person" class="contact-person" name="contact_person_m" type="radio" value="1"> Yes
                <input id="contact_person" class="contact-person" name="contact_person_m" checked="" type="radio" value="0"> No
            </div>
        </div>
        </fieldset>
    </div>
</div>